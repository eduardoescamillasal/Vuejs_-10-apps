n      dcHeroes.value = dcHeroes.value.filter((hero,i) => i != index);\n    }\n\n    function addHero() {\n        if (newHero.value !== \"\") {\n          dcHeroes.value.unshift({ name: newHero.value });\n          newHero.value = \"\";\n        }\n      }\n\n    return {dcHeroes, newHero, remove, addHero, newHeroRef, heroesCount};\n  },\n/*  computed: {\n    heroesCount() {\n      return this.dcHeroes.length;\n    },\n  },\n  methods: {\n  /*  addHero() {\n      if (this.newHero !== \"\") {\n        this.dcHeroes.unshift({ name: this.newHero });\n        this.newHero = \"\";\n      }\n    },*/\n  }\n/*  mounted() {\n    this.$refs.newHeroRef.focus();\n  },*/\n\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":""}]}