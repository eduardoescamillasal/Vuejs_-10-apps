oesCount }}</h1>\n      <ul>\n        <li class=\"flex justify-between\" v-for=\"(hero,index) in dcHeroes\" :key=\"hero.name\">\n          <div>\n            {{ hero.name }}\n          </div>\n          <button v-on:click=\"remove(index)\">x</button>\n        </li>\n      </ul>\n      <form class=\"mt-10\" @submit.prevent=\"addHero\">\n        <input class=\"border rounded\" v-model=\"newHero\" placeholder=\"Type Hero Name Here\" />\n        <button class=\"border rounded bg-gradient-to-r from-red-700 to-pink-500  text-white\" type=\"submit\">Add Hero</button>\n      </form>\n    </div>\n</template>\n\n<script>\nexport default {\n  computed: {\n    heroesCount() {\n      return this.dcHeroes.length;\n    },\n  },\n  methods: {\n    addHero() {\n      if (this.newHero !== \"\") {\n        this.dcHeroes.unshift({ name: this.newHero });\n        this.newHero = \"\";\n      }\n    },\n    remove (index) {\n      this.dcHeroes = this.dcHeroes.filter((hero,i) => i != index);\n    },\n  },\n  data(){\n    return {\n      newHero: \"\",\n      dcHeroes: [\n        {name: 'SuperGirl'},\n        {name: 'Flash'},\n        {name: 'Batman'},\n        {name: \"Arrow\"},\n        {name: \"Superman\"},\n      ],\n    };\n  },\n}\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":""}]}