AAA;AAAA,OAdM;AAAA;AAAA;;AAmBN,WAAO;AAAE,MAAA,KAAK,EAAL,KAAF;AAAS,MAAA,IAAI,EAAJ,IAAT;AAAe,MAAA,IAAG,EAAH;AAAf,KAAP;AACD;AArBY,CAAf","sourcesContent":["<template>\n  <section class=\"flex w-full\">\n    <div class=\"m-auto\">\n      <div class=\"mt-10\">\n        <table>\n          <thead>\n            <tr>\n              <th class=\"px-4 py-2 border\">ID</th>\n              <th class=\"px-4 py-2 border\">Avatar</th>\n              <th class=\"px-4 py-2 border\">First Name</th>\n              <th class=\"px-4 py-2 border\">Last Name</th>\n              <th class=\"px-4 py-2 border\">Email</th>\n            </tr>\n          </thead>\n          <tbody>\n            <tr v-for=\"user in state.users.data\" :key=\"user.id\">\n              <td class=\"border px-4 py-2\">{{ user.id}}</td>\n              <td class=\"border px-4 py-2\">\n                <img\n                  :src=\"user.avatar\"\n                  :alt=\"user.first_name\"\n                  width=\"50\"\n                  class=\"rounded-full\"\n                  />\n              </td>\n              <td class=\"border px-4 py-2\">{{ user.first_name }}</td>\n              <td class=\"border px-4 py-2\">{{ user.last_name }}</td>\n              <td class=\"border px-4 py-2\">{{ user.email }}</td>\n            </tr>\n          </tbody>\n        </table>\n        <div class=\"flex  justify-between\">\n          <button\n            class=\"px-3 py-2 border rounded hover:shadow\"\n            @click=\"prev\"\n            :disabled=\"state.users.page === 1\">\n              Prev\n          </button>\n          <button\n            class=\"px-3 py-2 border rounded hover:shadow\"\n            @click=\"next\">\n              Next\n          </button>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport { onMounted, reactive } from 'vue';\nimport axios from 'axios';\nexport default {\n  setup() {\n    const state = reactive({\n      users: {},\n    });\n    onMounted(async () => {\n      const { data } = await axios.get(\"https://reqres.in/api/users\");\n      state.users = data;\n    });\n\n     async function next() {\n      const { data } = await axios.get(\"https://reqres.in/api/users?page=2\");\n      state.users = data;\n    }\n\n    async function prev() {\n     const { data } = await axios.get(\"https://reqres.in/api/users?page=1\");\n     state.users = data;\n   }\n\n    return { state, next, prev };\n  },\n};\n</script>\n\n<style>\n\n</style>\n"],"sourceRoot":""}]}