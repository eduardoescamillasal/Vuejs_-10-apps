        email: 'ed.escamilla@bitfumes.com',\n        password: 'password',\n        isLoading: false,\n    };\n  },\n  mounted() {\n    //console.log(this.$refs);\n    //this.$refs.emailRef.focus();\n  },\n  methods: {\n    submit() {\n      //submit form\n      this.isLoading = true;\n      //use fo .default...\n      firebase\n      .default\n      .auth()\n      .signInWithEmailAndPassword(this.email, this.password)\n      .then(() => {\n        this.email = '';\n        this.password = '';\n        this.isLoading = false;\n        this.close();\n      })\n      .catch((e) => {\n        console.log(e);\n        this.isLoading = false;\n      });\n    },\n      close() {\n        this.$store.commit('setLoginModal', false);\n      },\n\n    },\n\n  };\n\n</script>\n\n<style>\n\n</style>\n"]}]}